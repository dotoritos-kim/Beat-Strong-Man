{
    "jsc": {
        "parser": {
            "syntax": "typescript",
            "tsx": true,
            "decorators": false
        },
        "target": "es2022",
        "transform": {
            "react": {
                "pragma": "h",
                "pragmaFrag": "Fragment",
                "throwIfNamespace": true,
                "development": false,
                "useBuiltins": false
            }
        }
    },
    "minify": true
}

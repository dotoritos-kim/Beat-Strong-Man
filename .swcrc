{
    "jsc": {
        "parser": {
            "syntax": "typescript",
            "tsx": true,
            "decorators": false
        },
        "target": "es2015",
        "transform": {
            "react": {
                "pragma": "h",
                "pragmaFrag": "Fragment",
                "throwIfNamespace": true,
                "development": false,
                "useBuiltins": false
            }
        }
    },
    "module": {
        "type": "commonjs",
        "strict": true,
        "noInterop": true
    },
    "minify": true
}

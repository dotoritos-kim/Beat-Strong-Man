{
    "jsc": {
        "parser": {
            "syntax": "typescript",
            "tsx": true,
            "decorators": false
        },
        "target": "es2022",
        "transform": {
            "react": {
                "runtime": "automatic"
            }
        }
    },
    "module": {
        "type": "commonjs",
        "strict": true,
        "noInterop": true
    },
    "minify": true
}
